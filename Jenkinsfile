pipeline {
	agent any
	environment {
      PATH = "D://apache-maven-3.6.3:$PATH"
		
 		 }
	stages {
		stage('Start') {
			steps {
				echo 'Starting'
			}
		}
		
		
		stage('Build') {
            steps {
		echo 'Ravi121212'
            	sh 'mvn install'
		   
            }
        }
        
        
		
		
	}
}
