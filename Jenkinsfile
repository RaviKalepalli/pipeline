pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Hi, I m in build stage'
            }
         stage('test') {
            steps {echo 'Hi, I m in test stage'
            }
         stage('deploy') {
            steps {
               echo 'Hi, I m in deploy stage'
            }
           
        }
    }
}
